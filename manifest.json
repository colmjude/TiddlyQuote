{
    "name": "Tiddly Quote",
    "description": "Create Tiddlers from Chrome",
    "version": "1.0.2",
    "options_page": "options.html",
    "permissions": [
        "activeTab",
        "http://tiddlyspace.com/",
        "http://*.tiddlyspace.com/"
    ],
    "browser_action": {
        "default_icon": "icon.png"  ,
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}