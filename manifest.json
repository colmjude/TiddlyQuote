{
  "manifest_version": 2,
  "name": "Tiddly Chrome",
  "description": "Create Tiddlers from Chrome",
  "version": "1.0",
  "options_page": "options.html",
  "permissions": [
    "http://tiddlyspace.com/",
    "http://*.tiddlyspace.com/",
    "http://*.tiddlyspace.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}